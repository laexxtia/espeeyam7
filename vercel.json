{
    "rewrites": [
      {
        "source": "/Login%20UI/(.*)", 
        "destination": "/Login%20UI/$1" 
      }
    ],
    "routes": [
      {
        "src": "/Login%20UI",
        "dest": "/Login%20UI/login.html"
      },
      {
        "src": "/(.*)",
        "status": 404,
        "continue": true
      }
    ]
  }
